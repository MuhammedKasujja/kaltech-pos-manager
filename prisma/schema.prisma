generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Account {
  id          Int    @id @default(autoincrement())
  account_key String
  expires_at  Int?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model DataUpload {
  id          Int    @id @default(autoincrement())
  account_key String
  device_id   String
  user_id     String
  data        String @db.LongText
  upload_id   Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
